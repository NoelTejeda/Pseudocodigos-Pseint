Algoritmo ejercicio_5
	Definir canhom,canmuj,Totalum,Porcehom,Porcemuj Como Real;
	Escribir 'Ingrese cantidad de hombres';
	Leer canhom;
	Escribir 'ingrese cantidad de mujeres';
	Leer canmuj;
	Totalum <- canhom+canmuj;
	Porcehom <- canhom*100/Totalum;
	Porcemuj <- canmuj*100/Totalum;
	Escribir 'porcentaje de hombres es ',Porcehom;
	Escribir 'porcentaje de mujeres es ',Porcemuj;
FinAlgoritmo

