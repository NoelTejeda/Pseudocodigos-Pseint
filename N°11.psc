Algoritmo Ejercicio_11
	Definir a,b,c,Prom Como Real;
	Escribir 'Ingrese los dias recorridos';
	Escribir 'Ingrese los minutos recorridos del lunes';
	Leer a;
	Escribir 'Ingrese los minutos recorridos del miercoles';
	Leer b;
	Escribir 'Ingrese los minutos recorridos del viernes';
	Leer c;
	Prom <- (a+b+c)/3;
	Escribir 'El promedio que recorres en la semana es de ',Prom,' Min';
FinAlgoritmo

