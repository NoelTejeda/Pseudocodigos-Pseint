Algoritmo ejercicio_29
	definir ncauchos como numerico;
	Escribir"Calculadora de descuentos";
	Escribir"Ingrese la cantidad de cauchos a comprar";
	leer ncauchos;
	
	si ncauchos<5 entonces
		
		Escribir"Usted Cancelarà " ncauchos*50000 " Bs";
		
	SiNo
		
